<!DOCTYPE html>
<html>
<head>
  <title>ShareSystem</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="window">
  <%= render "layouts/head" %>
  <div class="main">
    <div class="content">
      <%= yield %>
    </div>
    <%= render "layouts/footer" %> 
  </div>
</div>

</body>
</html>
